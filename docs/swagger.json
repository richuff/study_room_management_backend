                    }
                ],
                "responses": {
                    "200": {
                        "description": "业务代码",
                        "schema": {
                            "$ref": "#/definitions/result.CodeResp"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "dto.UserLoginDto": {
            "type": "object",
            "properties": {
                "email": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                }
            }
        },
        "dto.UserRegisterDto": {
            "type": "object",
            "properties": {
                "email": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                }
            }
        },
        "result.CodeResp": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "业务码，0=成功，非0=错误",
                    "type": "integer",
                    "example": 0
                },
                "msg": {
                    "description": "可读提示",
                    "type": "string",
                    "example": "success"
                }
            }
        }
    }
